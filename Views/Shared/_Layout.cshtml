<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Navbar -->
        @if (@HttpContextAccessor.HttpContext.Session.GetString("_Company") == "all" || @HttpContextAccessor.HttpContext.Session.GetString("_Company") == "LPS")
        {
            <partial name="Partial/Navbar_LPS"></partial>
            <partial name="Partial/Sidebar_LPS"></partial>
        }
        else 
        @if (@HttpContextAccessor.HttpContext.Session.GetString("_Company") == "BMW")
        {
            <partial name="Partial/Navbar_BMW"></partial>
            <partial name="Partial/Sidebar_BMW"></partial>
        }

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <br>
            @RenderBody()
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
